{
  "name": "jslt-node",
  "version": "0.1.0",
  "description": "Jslt",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "cargo-cp-artifact -nc ${ARTIFACT_PATH:=./bin/index.node} -- cargo build -p jslt-node --message-format=json-render-diagnostics",
    "zigbuild": "cargo-cp-artifact -nc ${ARTIFACT_PATH:=./bin/index.node} -- cargo zigbuild -p jslt-node --message-format=json-render-diagnostics",
    "clean": "rm -rf bin",
    "install": "node ./check-bin.js || pnpm run build",
    "prepack": "pnpm run clean && make bin",
    "test": "cargo test"
  },
  "devDependencies": {
    "cargo-cp-artifact": "^0.1"
  },
  "engine": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DmitryDodzin/jslt.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/DmitryDodzin/jslt/issues"
  },
  "homepage": "https://github.com/DmitryDodzin/jslt#readme"
}